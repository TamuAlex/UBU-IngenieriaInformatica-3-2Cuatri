package pg02.c02;

public class MyThreadX implements Runnable {

	@Override
	public void run() {
		for (int i = 0; i < 100000; i++) {
			
			if (i == 2) {
				Thread.yield();
			}
			System.out.print("x");
			
			try {
				Thread.sleep(1500);
			} catch (InterruptedException e) {
				e.printStackTrace();
			}
		}
		
	}

}
